package FiguresGeometriques;

public class Quadrat extends FiguraGeometrica{
	
	private int amplada;
	private int altura;
	private int area;
	private int perimetre;
	
	
	public Quadrat(int posicioX, int posicioY, int amplada, int altura) {
		super(posicioX, posicioY);
		this.amplada = amplada;
		this.altura = altura;
	}
	

}
