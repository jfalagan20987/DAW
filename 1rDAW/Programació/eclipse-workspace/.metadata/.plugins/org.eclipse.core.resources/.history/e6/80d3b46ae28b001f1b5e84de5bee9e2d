package NewPackage;
public class Linia {
	public static void main (String[] args) {
//Inicialitzem un comptador
int i = 0;
//Ja hem fet això 100 cops?
while (i < 100) {
System.out.print('-');
//Ho hem fet un cop, sumem 1 al comptador
i = i + 1;
}
//Forcem un salt de línia
System.out.println();
}

}