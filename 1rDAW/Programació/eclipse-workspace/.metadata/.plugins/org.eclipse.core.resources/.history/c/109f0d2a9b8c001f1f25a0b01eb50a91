package NewPackage;

import java.util.Scanner;

public class sentenciaFor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		final int NUMERO = 5;
		
		for (int i = 0 ; i<=10 ; i++) {
			System.out.println(NUMERO + " x " + i + " = " + (NUMERO*i));
		}

	}

}
