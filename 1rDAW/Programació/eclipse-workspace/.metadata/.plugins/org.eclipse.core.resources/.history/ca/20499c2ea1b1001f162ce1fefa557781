package Data;

public class DataResolt {
	
	private int dia;
	private int mes;
	private int any;
	
	public DataResolt(int dia, int mes, int any) {
		super();
		this.dia = dia;
		this.mes = mes;
		this.any = any;
		
		if (!validarData()) {
			this.dia = 0;
			this.mes = 0;
			this.any = 0;
			System.out.println("Data no vÃ lida!");
		};
	}
	
	private boolean validarData() {
		return false;
	}

}
