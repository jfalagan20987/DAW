package NewPackage;

public class IntercanviValors {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numA = 58;
		int numB = 7;
		int numC = numA - numB;
		
		System.out.print("numA és igual a: ");
		System.out.println(numA-=numC);
		
		System.out.print("numB és igual a: ");
		System.out.println(numB+=numC);

	}

}
