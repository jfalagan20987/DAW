package Llibres;

public class ArmariLlibres{
	
	Llibre[] llibres = new Llibre[10];
	int index = 0;
	
	public void afegirLlibre(Llibre llibre) {
		llibres[index] = llibre;
		index++;
	}
	
	public void eliminarLlibre(Llibre llibre) {
		llibres[index] = null;
	}
	
	public void modificarLlibre(Llibre llibre) {
		
	}

}
