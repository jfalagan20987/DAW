package NewPackage;

import java.util.Scanner;

public class MostraInterval_Exercici4_13 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		int n1=0, n2=0;
		
		do {
			System.out.println("Introdueix dos nombres, primer el més petit i després el més gran:");
			n1 = sc.nextInt();
			n2 = sc.nextInt();
		} while (n1 > n2);
			System.out.println("ERROR! El primer nombre no pot ser més gran que el segon. Torna a entrar la dada:");
			
		
		for (int interval = n1; interval <= n2 ; n1++) {
			System.out.println("El nombres entre " +n1+ " i " +n2+ " són: ");
		}
		
		sc.close();

	}

}
